<%-include('../partials/header');%>

<form action="/login" method="POST" id="register">

    <h1>Login</h1>

        <fieldset class="required">
            <legend for="username">Username</legend>
            <input type="text" name="username" id="username">
        </fieldset>
        
        <fieldset class="required">
            <legend for="psw">Password</legend>
            <input type="password" name="psw" id="psw">
        </fieldset>
<span>Don't have an acount? <a href="/register">Register now!</a></span>
        <input type="submit" class="btn" id="submit" value="Sign in">

    </form>

    <%-include('../partials/footer');%>